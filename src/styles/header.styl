


header
  .relative
    position: relative
  &.container
    padding: 3rem 0
  section
    // border: 1px solid
  .banner
    position: absolute
    bottom: 0
    padding: 0
    margin: 0
    img
      display: block
      height: 4rem
      max-width: 100%
      max-height: 100%

.header-links
  float: right;
  margin-right: 3em;
  a
    margin: 0 2rem
    display: inline-block
    color: rgb(130, 130, 130)
    font-size: 2.5rem
    text-decoration: none
    vertical-align: middle;
