$mobile = 320px
$tablet = 436px
$screen = 769px



$gutter = 0 //Unit represnt percentage

// ****** Functions and Mixins ********

/* Calculates column size percentage  based on a 12 column layout*/
column-size(colSpan)
  100 * colSpan/12 - $gutter + $gutter * colSpan/12

column-offset(colOffset)
  round(((column-size(1) + $gutter) * colOffset),2)
